server {
    listen 80;
    server_name timeoff.fr www.timeoff.fr;

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/tcher/timeoff/timeoff.sock;
    }

    location /static {
        alias /home/tcher/timeoff/app/static;
        expires 30d;
        add_header Cache-Control "public, immutable";
    }
}
